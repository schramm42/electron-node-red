{
    "name": "electron-node-red",
    "version": "1.0.2",
    "description": "Electron Node-RED application starter",
    "author": "zuhito",
    "main": "main.js",
    "scripts": {
        "start": "electron main.js",
        "build": "node node_modules/electron-builder/out/cli/cli.js --platform all --arch all",
        "build:osx": "node node_modules/electron-builder/out/cli/cli.js --mac --x64",
        "build:linux32": "node node_modules/electron-builder/out/cli/cli.js --linux --ia32",
        "build:linux64": "node node_modules/electron-builder/out/cli/cli.js --linux --x64",
        "build:win32": "node node_modules/electron-builder/out/cli/cli.js --win --ia32",
        "build:win64": "node node_modules/electron-builder/out/cli/cli.js --win --x64"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/zuhito/electron-node-red.git"
    },
    "dependencies": {
        "@ais_automation/node-red-contrib-odbc": "*",
        "express": "*",
        "node-red": "*",
        "node-red-contrib-amqp": "^1.0.2",
        "node-red-contrib-bigcsv": "^1.2.0",
        "node-red-contrib-cron-plus": "^1.3.0",
        "node-red-contrib-data-mapper": "^1.3.5",
        "node-red-contrib-fast-csv": "^0.4.0",
        "node-red-contrib-json2csv": "^0.0.3",
        "node-red-contrib-mapper": "^0.0.1",
        "node-red-contrib-mssql-plus": "^0.6.1",
        "node-red-contrib-play-audio": "*",
        "node-red-contrib-redis": "^1.3.8",
        "node-red-contrib-rsync": "*",
        "node-red-contrib-scp": "^0.1.4",
        "node-red-contrib-smb": "*",
        "node-red-contrib-stackhero-mysql": "^1.0.3",
        "node-red-contrib-string": "^1.0.0",
        "node-red-contrib-web-worldmap": "*",
        "node-red-dashboard": "*",
        "node-red-node-base64": "*",
        "node-red-node-email": "^1.8.2",
        "node-red-node-geofence": "*",
        "node-red-node-random": "*",
        "node-red-node-smooth": "*"
    },
    "keywords": [
        "electron",
        "node-red"
    ],
    "contributors": [
        {
            "name": "Dave Conway-Jones"
        },
        {
            "name": "Nathanaël Lécaudé"
        },
        {
            "name": "zuhito"
        }
    ],
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/zuhito/electron-node-red/issues"
    },
    "homepage": "https://github.com/zuhito/electron-node-red#readme",
    "devDependencies": {
        "electron": "*",
        "electron-builder": "*"
    }
}
